#include<stdio.h>
int main()
{
char a;
printf("Enter character:");
scanf("%c",&a);
switch(a)
{
case 'a':
printf("It is A vowel");
break;
case 'e':
printf("It is E vowel");
break;
case 'i':
printf("It is I vowel");
break;
case 'o':
printf("It is O vowel");
break;
case 'u':
printf("It is U vowel");
break;
default:
printf("It is a Consonant");
}
}

