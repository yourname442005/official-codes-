#include<stdio.h>
int main()
{
int marks;
printf("Enter Marks:\n");
scanf("%d",&marks);
if((marks>100)||(marks<0))
printf("WRONG ENTRY TRY AGAIN");
else if((marks<=100)&&(marks>=95))
printf("Grade is:AA");
else if((marks<95)&&(marks>=85))
printf("Grade is:A+");
else if((marks<85)&&(marks>=70))
printf("Grade is:A");
else if((marks<70)&&(marks>=60))
printf("Grade is:B");
else if((marks<60)&&(marks>=45))
printf("Grade is:C");
else
printf("Grade is:D");
}
